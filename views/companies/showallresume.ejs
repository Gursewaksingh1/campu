<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>

<body>
    <%- include('../includes/companynav.ejs') %>

    <main>
        <% if (prods.length > 0) { %>
            <div class="grid">
                
                    <table border="1px solid black">
                        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Stream</th>
            <th>InWhichYear</th>
            <th>Student-Resume</th>
            <th>Select-Student </th>
        </tr>
        <% for (let Student of prods) { %>
        <tr>
            <td><%=Student.name%></td>
            <td><%=Student.email%></td>
            <td><%=Student.stream%></td>
            <td><%=Student.inWhichYear%></td>
            <td><a href="/company/all-resume/<%=Student._id%>">resume</a></td>
            <td><a href="/company/select-student/<%=Student._id%>">select</a></td>
            <% } %>
</table>


</div>
<% } else { %>
<h1>No company Found!</h1>
<% } %>
</main>
<%- include('../includes/end.ejs') %>