<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>

<body>
    <%- include('../includes/companynav.ejs') %>

    <main>
        <% if (prods.length > 0) { %>
            <div class="grid">
                
                    <table border="1px solid black">
                        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Stream </th>
            
        </tr>
        <% for (let Student of prods) { %>
        <tr>
            <td><%=Student.name%></td>
            <td><%=Student.email%></td>
            <td><%=Student.stream%></td>
            <% } %>
</table>
</div>
<% } else { %>
<h1>No student selected!</h1>
<% } %>
</main>
<%- include('../includes/end.ejs') %>